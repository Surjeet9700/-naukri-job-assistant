{
  "name": "naukri-job-assistant",
  "version": "1.0.0",
  "description": "A browser extension that automates job applications on Naukri.com",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"cd backend && npm run dev\" \"cd extension && npm run dev\"",
    "build": "cd extension && npm run build",
    "start": "cd backend && npm run start",
    "test": "jest"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "pdfjs-dist": "^4.0.189"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "@types/pdf-parse": "^1.1.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
